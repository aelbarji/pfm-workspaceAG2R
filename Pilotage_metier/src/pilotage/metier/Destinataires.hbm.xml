<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD//EN"
                                   "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="pilotage.metier">

    <class name="pilotage.metier.Destinataires" table="destinataires">
       
        <id column="id" name="id" type="integer">
			<generator class="native" />
		</id>
        
        <property 	name="nom" 		column="nom" 		type="string" 			not-null="true"		length="127"		/>
        <property 	name="prenom"	column="prenom" 	type="string" 			not-null="true"		length="127"		/>
        <property 	name="mail"		column="mail" 		type="string" 			not-null="true"		length="127"		/>


    </class>
</hibernate-mapping>
