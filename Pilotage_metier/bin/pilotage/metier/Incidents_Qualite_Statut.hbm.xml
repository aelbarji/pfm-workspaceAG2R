<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >

<hibernate-mapping>
	<class name="pilotage.metier.Incidents_Qualite_Statut" table="INCIDENTS_QUALITE_STATUT">
		<id name="id" type="java.lang.Integer">
			<column name="ID"/>
			<generator class="native"/>
		</id>

		<property name="statut" type="java.lang.String" not-null="true">
			<column name="STATUT"/>
		</property>
	</class>	
</hibernate-mapping>