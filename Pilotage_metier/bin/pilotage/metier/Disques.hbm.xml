<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="pilotage.metier.Disques" table="DISQUES">
		<id name="id" type="java.lang.Integer" column="ID">
			<generator class="native" />
		</id>
		<property name="libelle"	column="LIBELLE" 	type="java.lang.String" not-null="true"/>
		<property name="filiale" 	column="FILIALE"	type="boolean" 			not-null="true"/>
		<property name="seuil" 		column="SEUIL"		type="float" 			not-null="true"/>
		<property name="deleted"	column="DELETED" 	type="boolean" 			not-null="true"/>
	</class>
</hibernate-mapping>
