import java.io.Serializable;
import java.util.Date;

public class Forum implements Serializable{
	private static final long serialVersionUID = 3760189257714351846L;

	private Integer id;
	private String theme;
	private Integer position;
	private Date    dateCreation;
	private Integer lastPost;
	
	/**
	 * Return the unique identifier of this class
     * @hibernate.id
     *  generator-class="org.hibernate.id.Assigned"
     *  column="id"
     */
	public Integer getId() {
		return id;
	}
	/**
	 * Set the unique identifier of this class
	 * @param id the new ID
	 */
	public void setId(Integer id) {
		this.id = id;
	}
	/**
	 * Return the value associated with the column: theme
	 */
	public String getTheme() {
		return theme;
	}
	/**
	 * Set the value related to the column: theme
	 * @param theme the theme value
	 */
	public void setTheme(String theme) {
		this.theme = theme;
	}
	/**
	 * Return the value associated with the column: position
	 */
	public Integer getPosition() {
		return position;
	}
	/**
	 * Set the value related to the column: position
	 * @param position the position value
	 */
	public void setPosition(Integer position) {
		this.position = position;
	}
	/**
	 * Return the value associated with the column: dateCreation
	 */
	public Date getDateCreation() {
		return dateCreation;
	}
	/**
	 * Set the value related to the column: dateCreation
	 * @param dateCreation the dateCreation value
	 */
	public void setDateCreation(Date dateCreation) {
		this.dateCreation = dateCreation;
	}
	/**
	 * Return the value associated with the column: lastPost
	 */
	public Integer getLastPost() {
		return lastPost;
	}
	/**
	 * Set the value related to the column: lastPost
	 * @param lastPost the lastPost value
	 */
	public void setLastPost(Integer lastPost) {
		this.lastPost = lastPost;
	}
}
