package pilotage.metier;

import java.io.Serializable;

import org.joda.time.DateTime;

public class Incidents_Gup implements Serializable {

	private static final long serialVersionUID = 1785971549719890207L;
	
	private Integer id;
	private Users user;
	private DateTime date_deb;
	private DateTime date_fin;
	private String description_prob;
	private Integer impact;
	private Com_Incident_Etat etat;
	private String ars;
	
	public Integer getId() {
		return id;
	}
	public void setId(Integer id) {
		this.id = id;
	}
	public Users getUser() {
		return user;
	}
	public void setUser(Users user) {
		this.user = user;
	}
	
	public DateTime getDate_deb() {
		return date_deb;
	}
	public void setDate_deb(DateTime date_deb) {
		this.date_deb = date_deb;
	}
	public DateTime getDate_fin() {
		return date_fin;
	}
	public void setDate_fin(DateTime date_fin) {
		this.date_fin = date_fin;
	}
	
	public String getDescription_prob() {
		return description_prob;
	}
	public void setDescription_prob(String description_prob) {
		this.description_prob = description_prob;
	}
	
	public Integer getImpact() {
		return impact;
	}
	public void setImpact(Integer impact) {
		this.impact = impact;
	}
	public Com_Incident_Etat getEtat() {
		return etat;
	}
	public void setEtat(Com_Incident_Etat etat) {
		this.etat = etat;
	}
	public String getArs() {
		return ars;
	}
	public void setArs(String ars) {
		this.ars = ars;
	}

}
